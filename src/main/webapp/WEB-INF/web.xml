<web-app>
	<display-name>Streaming demo</display-name>
	<servlet>
		<servlet-name>s</servlet-name>
		<servlet-class>org.test.streaming.Demo</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>s</servlet-name>
